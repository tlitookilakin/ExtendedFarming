<Project Sdk="Microsoft.NET.Sdk">
	
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<AssemblyName>ExtendedFarming</AssemblyName>
		<EnableHarmony>true</EnableHarmony>
		<Version>1.0.0</Version>
	</PropertyGroup>

	<PropertyGroup>
		<Name>Extended Farming</Name>
		<Authors>Tlitookilakin</Authors>
		<Description>Extends the functionality of stardew's data assets</Description>
		<UniqueId>tlitookilakin.extendedFarming</UniqueId>
		<MinimumApiVersion_Behavior>Update</MinimumApiVersion_Behavior>
		<UpdateKeys></UpdateKeys>
	</PropertyGroup>

	<ItemGroup>
		<None Include="i18n\" />
		<Using Include="StardewValley.Object" Alias="SObject" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Leclair.Stardew.ModManifestBuilder" Version="2.1.0" />
		<PackageReference Include="Pathoschild.Stardew.ModBuildConfig" Version="4.1.1" />
		<PackageReference Include="Pathoschild.Stardew.ModTranslationClassBuilder" Version="2.0.1" />
	</ItemGroup>

</Project>
